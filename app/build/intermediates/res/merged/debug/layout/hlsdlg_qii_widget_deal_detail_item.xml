<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/quotre_bid_sell_table"
    android:layout_width="fill_parent"
    android:orientation="horizontal"
    android:paddingLeft="2dip"
    android:paddingRight="2dip"
    android:layout_height="fill_parent" >

    <TextView
        android:id="@+id/quote_deal_time"
        android:layout_width="40dip"
        android:paddingRight="10dip"
        android:textSize="@dimen/font_size_11"
        android:gravity="left|center_vertical"
        android:layout_height="fill_parent" />

    <TextView
        android:id="@+id/quote_deal_price"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:textSize="@dimen/font_size_11"
        android:gravity="center"
        android:layout_weight="1" />

    <TextView
        android:id="@+id/quote_deal_volume"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:textSize="@dimen/font_size_11"
        android:gravity="right|center_vertical"
        android:layout_weight="1" />

</LinearLayout><!-- From: file:/Users/lxy/Desktop/hs/app/build/intermediates/exploded-aar/quotewidget/res/layout/hlsdlg_qii_widget_deal_detail_item.xml -->