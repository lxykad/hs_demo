<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/hlsdb_ten_buy_sell"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <include
            layout="@layout/hlsdb_widget_level2_portrait_offer"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
        <include
            layout="@layout/hlsdb_widget_level2_portrait_bid"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="13dp"
        android:background="#F5F5F5"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/hlsdb_fenbi_detail_title_tv"
        android:paddingLeft="10dp"
        android:gravity="center_vertical"
        android:text="分笔明细"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:src="@color/gray"/>
    <LinearLayout
        android:id="@+id/hlsdb_fenbi_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2">
        <include
            layout="@layout/hlsdb_widget_level2_zhubi_left"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
        <include
            layout="@layout/hlsdb_widget_level2_zhubi_right"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
    </LinearLayout>

</merge><!-- From: file:/Users/lxy/Desktop/hs/app/build/intermediates/exploded-aar/quotewidget/res/layout/hlsdb_widget_level1.xml -->