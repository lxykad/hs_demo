<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/hlkb_StockSearchInputEtLL"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:orientation="vertical">

        <EditText
            android:id="@+id/hlkb_qw_code_edit"
            android:layout_width="fill_parent"
            android:layout_height="46dip"
            android:layout_margin="4dip"
            android:hint="请输入代码"
            android:singleLine="true"
            android:textColor="#000000"></EditText>
    </LinearLayout>

    <ListView
        android:id="@+id/hlkb_qw_stock_list"
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_weight="1"
        android:cacheColorHint="#00000000"
        android:layout_above="@+id/hlkb_qw_keyboardview"
        android:layout_below="@+id/hlkb_StockSearchInputEtLL"
        android:divider="@drawable/hlkb_common_list_divider"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:scrollbars="vertical"></ListView>

    <com.hundsun.quotewidget.widget.QwKeyboardView
        android:id="@+id/hlkb_qw_keyboardview"
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_height="180dip">
    </com.hundsun.quotewidget.widget.QwKeyboardView>

</RelativeLayout><!-- From: file:/Users/lxy/Desktop/hs/app/build/intermediates/exploded-aar/quotewidget/res/layout/hlkb_qw_widget_search_view.xml -->