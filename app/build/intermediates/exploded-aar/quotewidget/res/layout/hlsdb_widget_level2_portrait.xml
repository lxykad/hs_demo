    <LinearLayout
        android:id="@+id/hlsdb_ten_buy_sell"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <include
            layout="@layout/hlsdb_widget_level2_portrait_offer"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
        <include
            layout="@layout/hlsdb_widget_level2_portrait_bid"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/hlsdb_ten_buy_sell_detail_title"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:weightSum="2">
        <TextView
            android:id="@+id/hlsdb_widget_level2_detail_title_sell"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="13dp"
            android:textSize="14dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="@string/hlsdb_offer1_label"/>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1">
            <TextView
                android:id="@+id/hlsdb_widget_level2_detail_title_buy"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:layout_marginLeft="9dp"
                android:textSize="14dp"
                android:text="@string/hlsdb_bid1_label"
                android:layout_gravity="center_vertical"/>
            <ImageView
                android:id="@+id/hlsdb_widget_level2_detail_title_imagebtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="9dp"
                android:background="@drawable/hlsdb_level2_detail_imgbtn"/>
        </RelativeLayout>
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:src="@color/gray"/>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/hlsdb_level2_portrait_total_perbuy"
        android:weightSum="2">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"
            android:layout_weight="1">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:id="@+id/hlsdb_level2_portrait_total_sell"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textSize="14dp"
                android:textStyle="normal"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:id="@+id/hlsdb_level2_portrait_sell_perhand"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textSize="14dp"
                android:textStyle="bold"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"
            android:layout_weight="1">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:id="@+id/hlsdb_level2_portrait_total_buy"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textSize="14dp"
                android:textStyle="normal"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:id="@+id/hlsdb_level2_portrait_buy_perhand"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:textSize="14dp"
                android:textStyle="bold"/>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/hlsdb_ten_buy_sell_detail"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:id="@+id/hlsdb_selllist_tv"
            style="@style/hlsdb_level2_label_Text_Style"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="9dp"
            android:paddingRight="9dp"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:includeFontPadding="false"
            android:minLines="3"
            android:textColor="@color/hlsdb_stock_name_color"
            android:textSize="13dp"/>
        <TextView
            android:id="@+id/hlsdb_buylist_tv"
            style="@style/hlsdb_level2_label_Text_Style"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="9dp"
            android:paddingRight="9dp"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:layout_weight="1"
            android:includeFontPadding="false"
            android:minLines="3"
            android:textColor="@color/hlsdb_stock_name_color"
            android:textSize="13dp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="13dp"
        android:background="#F5F5F5"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/hlsdb_zhubi_detail_title">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:id="@+id/hlsdb_zhubi_detail_title_tv"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:text="逐笔明细"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/hlsdb_zhubi_detail_title_iv"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="9dp"
            android:background="@drawable/hlsdb_level2_detail_imgbtn"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:src="@color/gray"/>
    <LinearLayout
        android:id="@+id/zhubi_detail"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2">
        <include
            layout="@layout/hlsdb_widget_level2_zhubi_left"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
        <include
            layout="@layout/hlsdb_widget_level2_zhubi_right"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"/>
    </LinearLayout>

</LinearLayout>